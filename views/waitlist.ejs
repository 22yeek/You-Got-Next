        </div>

        <div class="court-name"><%= court_name %></div>
        <div class="left-name"><%= left_name %></div>
          
        <div class="left-logo">
            <img src="/logos/<%= left_logo %>" alt="<%= left_name %> logo">
        </div>

        <div class="vs">vs</div>
        <div class="right-name"><%= right_name %></div>

        <div class="right-logo">
            <img src="/logos/<%= right_logo %>" alt="<%= right_name %> logo">
        </div>
        <div class = "queue">
            <h2>Teams in Queue</h2>
                <ul>
                    <% teams.forEach(function(team) { %>
                        <li>Team <%= team.tname %> (# of Players: <%= team.count %>)</li>
                    <% }) %>
                </ul>
        </div>
        <div class="est-wait"><%= est_wait %></div>
        <div class="hop-in-line-button">
            <span>Hop in line</span>
        </div>

        <div class="grey-line"></div>
        <div class="team-text">Team</div>
        <div class="size-text">Size</div>
    </div>
    <button onclick="window.location.href='/signup'">Create New Team</button>
    <!-- Link the external waitScript file -->
    <script src="/scripts/waitScript.js"></script>
</body>
</html>


